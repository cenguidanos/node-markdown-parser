    "rehype-format": "^3.1.0",
    "rehype-katex": "^3.0.0",
    "rehype-raw": "^4.0.2",
    "rehype-stringify": "^8.0.0",
    "remark-attr": "^0.11.1",
    "remark-emoji": "^2.1.0",
    "remark-math": "^2.0.1",
    "remark-parse": "^8.0.3",
